const CounterUi = ({Count,inc,dec}) =>{
    return(
        <>
        <div className="col-lg-6 mx-auto my-5 p-5 shadow">
            <h2>{Count}</h2>
            <button className="btn btn-outline-primary " onClick={inc}>+</button>
            <button className="btn btn-outline-danger mx-2" onClick={dec}>-</button>

        </div>
        </>
    )
}

export default CounterUi